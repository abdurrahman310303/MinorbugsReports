{
    "version": 2,
    "builds": [
      {
        "src": "applovin_reports/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime":"python3.13.3"}
      },
      {
        "src": "build.sh",
        "use": "@vercel/python",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "applovin_reports/wsgi.py"
      }
    ]
  }
  